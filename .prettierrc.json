{
    "arrowParens": "avoid",
    "singleQuote": true,
    "printWidth": 90,
    "semi": false,
    "trailingComma": "none",
    "plugins": ["prettier-plugin-astro"],
    "overrides": [
      {
        "files": "**/*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  }
  